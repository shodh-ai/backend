{
  "family": "backend-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::302263050318:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "backend-container",
      "image": "302263050318.dkr.ecr.ap-south-1.amazonaws.com/backend-api:latest1",
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "PORT",
          "value": "8080"
        },
        {
          "name": "POSTGRES_USERNAME",
          "value": "postgres"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "shodhAIpostgres"
        },
        {
          "name": "ML_URL",
          "value": "https://3m4szz0n9l.execute-api.ap-south-1.amazonaws.com/Prod"
        },
        {
          "name": "DDL_AUTO",
          "value": "update"
        },
        {
          "name": "SHOW_SQL",
          "value": "false"
        },
        {
          "name": "CLOUDINARY_CLOUD_NAME",
          "value": "damg6iidi"
        },
        {
          "name": "CLOUDINARY_CLOUD_API_KEY",
          "value": "554238382312718"
        },
        {
          "name": "CLOUDINARY_CLOUD_SECRET",
          "value": "WLS-smwAXQUupVHlI1q-15L0mCo"
        },
        {
          "name": "MAX_FILE_SIZE",
          "value": "20MB"
        },
        {
          "name": "MAX_REQUEST_SIZE",
          "value": "20MB"
        },
        {
          "name": "MAIL_HOST",
          "value": "smtp.gmail.com"
        },
        {
          "name": "MAIL_PORT",
          "value": "587"
        },
        {
